{
	"name": "blazorcomponents",
	"displayName": "BlazorComponents",
	"description": "Create seperate components",
	"version": "0.0.6",
	"publisher": "tHop",
	"type": "git",
	"repository": "https://dev.azure.com/thomashhopp/_git/BlazorComponents",
	"engines": {
		"vscode": "^1.37.0"
	},
	"categories": [
		"Other"
	],
	"main": "./out/extension.js",
	"activationEvents": [
		"onCommand:blazorcomponents.createComponent"
	],
	"contributes": {
		"commands": [
			{
				"command": "blazorcomponents.createComponent",
				"title": "New Blazor component"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "blazorcomponents.createComponent",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.37.0",
		"glob": "^7.1.4",
		"mocha": "^7.1.2",
		"tslint": "^5.20.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	}
}
